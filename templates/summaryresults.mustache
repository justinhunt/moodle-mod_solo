
    {{! Transcript evaluation }}
    <div>
        {{#autotranscriptready}}
        <div class='mod_solo_sr_title'>{{#str}}transcriptevaluation, mod_solo{{/str}}</div>
        <div class='mod_solo_sr_details'>{{#str}}transcriptevaluationdetails, mod_solo{{/str}}</div>
        {{/autotranscriptready}}
        {{^autotranscriptready}}
            <div class='mod_solo_sr_title'>{{#str}}selftranscript, mod_solo{{/str}}</div>
        {{/autotranscriptready}}
        <div class='mod_solo_ct_body'>
            <div class="mod_solo_ct_audioplayer">{{>mod_solo/audioplayer}}</div>
            {{{markedpassage}}}
        </div>
    </div>
    <hr>
    {{#grammarcorrection}}
        <div class='mod_solo_sr_title'>{{#str}}grammarcorrection, mod_solo{{/str}}</div>
        <div class='mod_solo_sr_details mod_solo_corrections_cont'>{{{grammarcorrection}}}</div>
        <hr>
    {{/grammarcorrection}}

    <div class="mod_solo_attemptdetailscontainer">
        <div id="attemptdetails">
            <div>
                <div class='mod_solo_sr_title'>{{#str}}stats, mod_solo{{/str}}</div>
                {{>mod_solo/summarystats}}
            </div>
        
        {{! Spelling evaluation }}
        {{#selftranscribe}}
            <div>
                <div class='mod_solo_sr_title'>{{#str}}spellingeval, mod_solo{{/str}}</div>
                {{>mod_solo/summaryspellingeval}}
            </div>
        {{/selftranscribe}}

        {{! Grammar evaluation }}
            <div>
                <div class='mod_solo_sr_title'>{{#str}}grammareval, mod_solo{{/str}}</div>
                {{>mod_solo/summarygrammareval}}
            </div>
        </div>
    </div>



